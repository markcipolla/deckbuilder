<%= cache ['cards#index', 'v1'], :expires_in => 1.hour do %>
  <% content_for :title, "Cards" %>
  <div class="row">
    <% @cards.each do |card| %>
      <div class="card col-xs-4 col-md-3">
        <%= link_to image_tag(card.card_image), card_path(card) %>
      </div>
    <% end %>
  </div>
<% end %>
